@model IdentityServer.ViewModels.Account.LogoutViewModel

@if (Model == default)
{
    <span class="lead">Loading ...</span>
    return;
}

<div class="logout-page">
    <partial name="_Title" model="@Model.TitleModel" />

    <form asp-action="Logout">
        <input type="hidden" name="logoutId" value="@Model?.LogoutId"/>
        <div class="form-group">
            <button class="btn btn-outline-secondary w-25">Yes</button>
        </div>
    </form>
</div>